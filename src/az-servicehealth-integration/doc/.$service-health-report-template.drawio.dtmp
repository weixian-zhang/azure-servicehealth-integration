        <mxCell id="stc5pnTG8elvpdlf7ydB-2" value="Post Incident Review (PIR) - Azure Resource Manager - Services impacted by ARM failures" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontStyle=1;fontSize=14;" parent="1" vertex="1">
