{
  "version": "2.0",
  "queues": {
    "maxDequeueCount": 1
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
          "isEnabled": true,
          "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "default": "Error",
      "Host.Results": "Error"
    }
  },
  "telemetryMode": "openTelemetry",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}